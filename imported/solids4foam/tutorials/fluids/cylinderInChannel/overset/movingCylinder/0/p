FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    OversetFaces
    {
        type overset;
        value uniform 0;
        coupledFringe yes;
        
        conservativeCorrection yes;
        
        setHoleCellValue  yes;
        holeCellValue     0;
    }
    TopWall
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            zeroGradient;
    }
    Outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    OuterCylinder
    {
        type            zeroGradient;
    }
    CylinderWall
    {
        type            zeroGradient;
    }
    BottomWall
    {
        type            zeroGradient;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}

