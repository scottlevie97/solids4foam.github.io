planeStress     no;

mechanical
(
    steelUMAT
    {
        type            abaqusUmatLinearElastic;
        rho             rho [1 -3 0 0 0 0 0] 7854;
        // "Implicit stiffness": affects convergence
        implicitStiffness implicitStiffness [1 -1 -2 0 0 0 0] 2.6923e+11;
        properties
        (
            200e+9  // E
            0.3     // nu
        );
    }
