FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     yes;
//writeSubMeshes  yes;

mechanical
(
    steel
    {
        type            linearElastic;
        rho             rho [1 -3 0 0 0 0 0] 7854;
        E               E [1 -1 -2 0 0 0 0] 200e+9;
        nu              nu [0 0 0 0 0 0 0] 0.3;
    }
    polymer
    {
        type            viscousHookeanElastic;
        rho             rho [1 -3 0 0 0 0 0] 7854;
        EInfinity       E [1 -1 -2 0 0 0 0] 2e+9;
        E               ( 1e+09 0.5e+09 0.1e+09 );
        relaxationTimes ( 100 10 1 );
        nu              nu [0 0 0 0 0 0 0] 0.3;
    }
);

